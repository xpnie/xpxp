FUNCTION FC1001 :BOOL
VAR_INPUT
	MEW: REAL;
	SP: REAL;
	HYS: REAL;
END_VAR
VAR_OUT
 	OUT:BOOL;
END_VAR
VAR
 	P_HYS: REAL;
END_VAR

OUT := FALSE;

IF (MEW > SP) THEN
  P_HYS := MEW - HYS * SP / 100;
  IF (P_HYS >= SP) THEN
    OUT := TRUE;
  END_IF;
ELSE
  P_HYS := MEW + HYS * SP / 100;
  IF (P_HYS <= SP) THEN
    OUT := FALSE;
  END_IF;
END_IF;

END_FUNCTION;